<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>TrabPoo</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
     <servlet>
        <servlet-name>servletAdd</servlet-name>
        <servlet-class>br.com.poo.servlet.AdicionaProduto</servlet-class>        
    </servlet>
    
    <servlet>
     	<servlet-name>servletBusca</servlet-name>
        <servlet-class>br.com.poo.servlet.BuscaProduto</servlet-class>
     </servlet>
     
      <servlet>
     	<servlet-name>servletEdita</servlet-name>
        <servlet-class>br.com.poo.servlet.EditaProduto</servlet-class>
     </servlet>
     
     <servlet>
     	<servlet-name>servletRemove</servlet-name>
        <servlet-class>br.com.poo.servlet.RemoveProduto</servlet-class>
     </servlet>
     
     <servlet>
     	<servlet-name>servletDownload</servlet-name>
        <servlet-class>br.com.poo.servlet.BaixaRelatorio</servlet-class>
     </servlet>
</web-app>